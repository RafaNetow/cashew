{
  "name": "cashew",
  "version": "0.0.1",
  "description": "Cashew is a real-time communications backend for connected applications.",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "main": "build/src/index.js",
  "files": [
    "build"
  ],
  "devDependencies": {
    "@types/jest": "~22.2.3",
    "@types/node": "~8.10.10",
    "jest": "~22.4.3",
    "rimraf": "~2.6.2",
    "ts-jest": "~22.4.4",
    "ts-node": "^6.0.3",
    "tslint": "~5.9.1",
    "tslint-microsoft-contrib": "~5.0.3",
    "tsutils": "~2.26.2",
    "typescript": "~2.8.3"
  },
  "scripts": {
    "prepare": "npm run build",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Byron Sommardahl <byron@acklenavenue.com>",
  "dependencies": {
    "aedes": "git+https://github.com/bsommardahl/aedes.git",
    "aedes-persistence-redis": "^5.1.0",
    "dotenv": "^5.0.1",
    "jsonwebtoken": "^8.2.1",
    "mqtt": "^2.18.0",
    "mqemitter-redis": "^2.3.0",
    "tslib": "~1.9.0",
    "winston-loggly-bulk": "^2.0.2"
  }
}
